{
  "name": "@slango/ts-utils",
  "version": "1.0.1",
  "private": false,
  "description": "Slango TS utilities",
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build:check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {},
  "peerDependencies": {
    "@vitest/coverage-v8": "^3",
    "@vitest/ui": "^3",
    "eslint": "^9",
    "typescript": "^5",
    "vitest": "^3"
  },
  "devDependencies": {
    "@slango.configs/eslint": "workspace:*",
    "@slango.configs/lint-staged": "workspace:*",
    "@slango.configs/typescript": "workspace:*",
    "@slango.configs/vitest": "workspace:*"
  }
}
