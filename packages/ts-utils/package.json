{
  "name": "@slango/ts-utils",
  "version": "1.0.0",
  "private": false,
  "description": "Slango TS utilities",
  "type": "module",
  "exports": {
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build:check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {},
  "peerDependencies": {
    "@vitest/coverage-v8": "3.0.9",
    "@vitest/ui": "3.0.9",
    "eslint": "9.23.0",
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "devDependencies": {
    "@slango.configs/eslint": "workspace:*",
    "@slango.configs/lint-staged": "workspace:*",
    "@slango.configs/typescript": "workspace:*",
    "@slango.configs/vitest": "workspace:*"
  }
}
