{
  "name": "@slango/mongoose",
  "version": "0.0.1",
  "private": false,
  "description": "slango mongoose middlewares and utilities",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:force": "tsc --incremental false -p tsconfig.build.json",
    "build:watch": "tsc --watch -p tsconfig.build.json",
    "build:check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "mongoose": "8.12.1",
    "nanoid": "5.1.5"
  },
  "peerDependencies": {
    "@vitest/coverage-v8": "3.0.9",
    "@vitest/ui": "3.0.9",
    "eslint": "9.23.0",
    "typescript": "5.8.2",
    "vitest": "3.0.9"
  },
  "devDependencies": {
    "@slango.configs/eslint": "workspace:*",
    "@slango.configs/typescript": "workspace:*",
    "@slango.configs/vitest": "workspace:*",
    "@types/bcrypt": "5.0.2"
  }
}
