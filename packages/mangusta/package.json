{
  "name": "@slango/mangusta",
  "version": "1.1.2",
  "private": false,
  "description": "Mongoose middlewares and utilities",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:force": "tsc --incremental false -p tsconfig.build.json",
    "build:watch": "tsc --watch -p tsconfig.build.json",
    "build:check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "bcrypt": "6.0.0",
    "nanoid": "5.1.6"
  },
  "peerDependencies": {
    "mongoose": "^8.19.0"
  },
  "devDependencies": {
    "@slango.configs/eslint": "workspace:*",
    "@slango.configs/typescript": "workspace:*",
    "@slango.configs/vitest": "workspace:*",
    "@types/bcrypt": "6.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "eslint": "9.37.0",
    "mongoose": "8.19.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  }
}
