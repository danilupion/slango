{
  "name": "@slango/mangusta",
  "version": "2.1.8",
  "private": false,
  "description": "Mongoose middlewares and utilities",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    },
    "./test-utils/*": {
      "import": "./dist/test-utils/*.js",
      "types": "./dist/test-utils/*.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:force": "tsc --incremental false -p tsconfig.build.json",
    "build:watch": "tsc --watch -p tsconfig.build.json",
    "build:check": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix --max-warnings 0",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "bcrypt": "6.0.0",
    "mongodb-memory-server": "11.0.1",
    "nanoid": "5.1.6"
  },
  "peerDependencies": {
    "mongoose": "^9.1.5",
    "vitest": "^4.0.17"
  },
  "peerDependenciesMeta": {
    "vitest": {
      "optional": true
    }
  },
  "devDependencies": {
    "@slango.configs/eslint": "workspace:*",
    "@slango.configs/typescript": "workspace:*",
    "@slango.configs/vitest": "workspace:*",
    "@types/bcrypt": "6.0.0",
    "@vitest/coverage-v8": "4.0.17",
    "@vitest/ui": "4.0.17",
    "eslint": "9.39.2",
    "mongoose": "9.1.5",
    "typescript": "5.9.3",
    "vitest": "4.0.17"
  }
}
